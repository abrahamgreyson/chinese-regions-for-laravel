{
    "name": "abe/chinese-regions-for-laravel",
    "description": "中国行政区划",
    "keywords": [
        "abe",
        "laravel",
        "chinese regions",
        "中国行政区划"
    ],
    "homepage": "https://github.com/abe/chinese-regions-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "abraham greyson",
            "email": "abrahamgreyson@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "ext-pdo_sqlite": "*",
        "spatie/laravel-package-tools": "^1.13",
        "illuminate/contracts": "^8|^9",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Abe\\LaravelChineseRegions\\": "src",
            "Abe\\LaravelRegions\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abe\\LaravelChineseRegions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "abe": {
            "wechat": "abrahamgreyson"
        },
        "laravel": {
            "providers": [
                "Abe\\ChineseRegions\\ChineseRegionsServiceProvider"
            ]
        }
    },
    "suggest": {
        "ext-pdo_sqlite": "使用该扩展解析来自数据源的 SQLite 数据库"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
